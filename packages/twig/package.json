  "scripts": {
    "dev:storybook": "node importprefix.js && node importsvgs.js && pnpm ws dev storybook",
    "build:storybook": "node importprefix.js && node importsvgs.js && pnpm ws build storybook",
    "docs:storybook": "node importprefix.js && node importsvgs.js && pnpm ws build storybook --docs",
    "storybook": "node importprefix.js && node importsvgs.js && start-storybook --config-dir apps/storybook",
    "build:docs": "node importprefix.js && node importsvgs.js && storybook build --config-dir apps/storybook -o ./storybook-static",
    "build:template": "node copytemplates.js",
    "build:styles": "node copystyles.js",
    "build:js": "rollup -c --bundleConfigAsCjs",
    "build:lib": "pnpm build:template && pnpm build:js && pnpm build:styles",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check . --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write . --ignore-path ../../.prettierignore",
    "import:prefix": "node importprefix.js",
    "import:svgs": "node importsvgs.js",
    "start:theme": "docker compose up -d",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "cypress run"
  },
  "dependencies": {
    "@ilo-org/brand-assets": "workspace:*",
