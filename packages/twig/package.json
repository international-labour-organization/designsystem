{
  "name": "@ilo-org/twig",
  "version": "0.2.12",
  "license": "Apache-2.0",
  "description": "Twig components for the ILO's Design System",
  "main": "",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/international-labour-organization/designsystem.git",
    "directory": "packages/twig"
  },
  "keywords": [
    "ui_patterns",
    "prototyping",
    "twig",
    "designsystem",
    "components"
  ],
  "scripts": {
    "storybook": "node importprefix.js; node importsvgs.js; start-storybook --config-dir apps/storybook",
    "build:docs": "node importprefix.js; node importsvgs.js; build-storybook --config-dir apps/storybook -o ./storybook-static",
    "build:lib": "pnpm output",
    "build": "pnpm build:lib",
    "lint:js": "eslint --ext .js ./",
    "lint:css": "stylelint '**/*.css'",
    "lint": "npm run lint:js; npm run lint:css;",
    "format": "prettier --check . --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write . --ignore-path ../../.prettierignore",
    "fmt:css": "npm run lint:css --fix && prettier --loglevel warn --write '**/*.{css}'",
    "fmt": "npm run fmt:js; npm run fmt:css",
    "ci": "npm run lint && npm run build:drupal",
    "import:prefix": "node importprefix.js",
    "import:svgs": "node importsvgs.js",
    "output": "node outputtwigs.js"
  },
  "dependencies": {
    "@ilo-org/fonts": "workspace:*",
    "@ilo-org/icons": "workspace:*",
    "@ilo-org/styles": "workspace:*",
    "@ilo-org/themes": "workspace:*",
    "@ilo-org/utils": "workspace:*",
    "@wingsuit-designsystem/preset-scss": "^1.2.7",
    "video.js": "^7.21.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.16",
    "@storybook/linter-config": "^2.5.0",
    "@storybook/react": "^6.5.16",
    "@storybook/storybook-deployer": "^2.8.16",
    "@types/react": "^16.14.35",
    "@types/react-dom": "^16.9.17",
    "@wingsuit-designsystem/cli": "1.2.7",
    "@wingsuit-designsystem/core": "1.2.7",
    "@wingsuit-designsystem/pattern": "1.2.7",
    "@wingsuit-designsystem/preset-scss": "^1.2.7",
    "@wingsuit-designsystem/storybook": "1.2.7",
    "autoprefixer": "^10.4.13",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.2.0",
    "fs": "0.0.1-security",
    "fs-extra": "^8.1.0",
    "glob": "^7.2.3",
    "jquery-once": "^2.2.3",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.6.2",
    "optimize-css-assets-webpack-plugin": "^5.0.8",
    "postcss": "^8.4.12",
    "postcss-loader": "^4.3.0",
    "postcss-nested": "^4.2.3",
    "postcss-scss": "^3.0.5",
    "raw-loader": "^4.0.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "regenerator-runtime": "^0.13.3",
    "require-context": "^1.1.0",
    "resolve-url-loader": "^3.1.5",
    "sass-loader": "^10.4.1",
    "storybook": "^6.5.16",
    "style-loader": "^1.3.0",
    "stylelint-scss": "^3.21.0",
    "webpack": "^4.46.0",
    "yaml-loader": "^0.6.0",
    "yo": "^3.1.1"
  }
}
