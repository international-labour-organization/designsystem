  "license": "Apache-2.0",
  "description": "Twig components for the ILO's Design System",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "homepage": "https://twig.ui.ilo.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/international-labour-organization/designsystem.git",
    "directory": "packages/twig"
  },
  "keywords": [
    "ui_patterns",
    "prototyping",
    "twig",
    "designsystem",
    "components"
  ],
  "contributors": [
    {
      "name": "Justin Smith",
      "email": "smithj@ilo.org",
