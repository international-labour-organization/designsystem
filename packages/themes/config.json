{
  "source": ["tokens/**/*.json"],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "buildPath": "build/scss/",
      "files": [{
        "destination": "_tokens.scss",
        "format": "scss/individualMaps"
      }],
      "transforms": ["attribute/cti", "color/rgb", "size/rem", "name/cti/kebab"]
    }
  }
}