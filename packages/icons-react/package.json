{
  "name": "@ilo-org/icons-react",
  "description": "React components for icons in digital and software products using ILO's Design System",
  "version": "0.0.10",
  "author": "@justintemps",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/international-labour-organization/designsystem.git",
    "directory": "packages/icons-react"
  },
  "bugs": "https://github.com/international-labour-organization/designsystem/issues",
  "files": [
    "es",
    "lib",
    "umd",
    "next"
  ],
  "keywords": [
    "elements",
    "components",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean && node tasks/build.js",
    "clean": "rimraf es lib umd next",
    "format": "prettier --check . --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write . --ignore-path ../../.prettierignore",
    "lint": "eslint --ext .js,.ts,.tsx",
    "lint:fix": "eslint --ext .js,.ts,.tsx --fix",
    "postinstall": "carbon-telemetry collect --install"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@carbon/icon-helpers": "^10.19.0",
    "@carbon/telemetry": "0.0.0-alpha.6",
    "@ilo-org/icon-build-helpers": "workspace:*",
    "@ilo-org/icons": "workspace:*"

  },
  "devDependencies": {
    "@babel/plugin-transform-react-constant-elements": "^7.17.6",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-plugin-dev-expression": "^0.2.3",
    "browserslist-config-carbon": "^10.6.1",
    "prop-types": "^15.7.2",
    "rimraf": "^3.0.2"
  },
  "sideEffects": false
}
