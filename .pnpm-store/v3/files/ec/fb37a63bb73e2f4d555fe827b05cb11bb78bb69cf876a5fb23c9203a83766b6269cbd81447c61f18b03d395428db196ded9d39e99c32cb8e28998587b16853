import { Channel } from '@storybook/channels';
import React from 'react';
import { PublicInteractionTask, AllowedGroup } from '../types';
declare type Props = {
    getNode: () => React.ReactNode;
    channel: Channel;
    interactions: PublicInteractionTask[];
    allowedGroups: AllowedGroup[];
};
export default function TaskHarness({ getNode, channel, interactions, allowedGroups }: Props): React.ReactElement<any, string | ((props: any) => React.ReactElement<any, any> | null) | (new (props: any) => React.Component<any, any, any>)>;
export {};
